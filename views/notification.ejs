        // timeline js 2 modal
        jQuery(document).ready(function($) {
            var $timeline_block = $('.cd-timeline-block');

            //hide timeline blocks which are outside the viewport
            $timeline_block.each(function() {
                if ($(this).offset().top > $(window).scrollTop() + $(window).height() * 0.75) {
                    $(this).find('.cd-timeline-img, .cd-timeline-content').addClass('is-hidden');
                }
            });

            //on scolling, show/animate timeline blocks when enter the viewport
            $(window).on('scroll', function() {
                $timeline_block.each(function() {
                    if ($(this).offset().top <= $(window).scrollTop() + $(window).height() * 0.75 && $(this).find('.cd-timeline-img').hasClass('is-hidden')) {
                        $(this).find('.cd-timeline-img, .cd-timeline-content').removeClass('is-hidden').addClass('bounce-in');
                    }
                });
            });
        });

        async function showLocation(data) {
            $('#cd-timeline').empty()
            if (data.length == 1) {
                await data.forEach(d => {
                    $('#cd-timeline').append(` <div class="cd-timeline-block">
                            <div class="cd-timeline-img cd-location">
                                <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/148866/cd-icon-location.svg" alt="Location">
                            </div>
                            <!-- cd-timeline-img -->
                            <div class="cd-timeline-content">
                                <a href="/map?latitude=${d.latitude}&longitude=${d.longitude}">Show on map</a>
                                
                                <span class="cd-date">out of Quarantine on : - ${d.date}</span>
                            </div>
                            <!-- cd-timeline-content -->
                        </div>`)
                })
            } else {
                $('#cd-timeline').append(` <div class="cd-timeline-block">
                            <div class="cd-timeline-img cd-location">
                                <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/148866/cd-icon-location.svg" alt="Location">
                            </div>
                            <!-- cd-timeline-img -->
                            <div class="cd-timeline-content">
                                <a href="#" onclick="sendLocationObj(<%=JSON.stringify(data)%>)">Show full route on map</a>
                                <span class="cd-date"></span>
                            </div>
                            <!-- cd-timeline-content -->
                        </div>`)
            }

        }

